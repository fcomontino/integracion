{
  "name": "proxy-server",
  "version": "0.1.0",
  "scripts": {
    "start": "nodemon -w ./src -x \"npm run dev\"",
    "babel": "babel src -d dist --copy-files",
    "build": "npm run clean && npm run babel",
    "clean": "rm -rf dist",
    "dev": "npm run build && node --icu-data-dir=node_modules/full-icu dist/index.js",
    "knex": "node_modules/.bin/knex --knexfile=./knexfile.js",
    "migrate": "node_modules/.bin/knex migrate:latest",
    "seed": "node_modules/.bin/knex seed:run",
    "db": "npm run migrate && npm run seed",
    "setup": "npm install && npm run db"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@sendgrid/mail": "^7.4.2",
    "@sentry/node": "^6.3.5",
    "@sentry/tracing": "^6.3.5",
    "axios": "^0.19.2",
    "dotenv": "^8.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "full-icu": "^1.3.1",
    "knex": "^0.21.1",
    "knex-paginate": "^2.1.0",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-ejs": "^4.3.0",
    "koa-json": "^2.0.2",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "nodemon": "^1.19.1",
    "pg": "^8.0.3",
    "qs": "^6.7.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  }
}
